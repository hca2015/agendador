@model Tcc.Models.CadastrarCNPJ
<title>Agendador | Cadastrar CNPJ</title>

@using (Html.BeginForm("CadastrarCNPJIncluir", "CNPJ", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    <h4>Entre com suas informações.</h4>
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(m => m.NomeFantasia, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.NomeFantasia, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.RazaoSocial, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.RazaoSocial, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
    @Html.LabelFor(m => m.CNPJ, new { @class = "col-md-2 control-label" })
    <div class="col-md-10">
        @Html.TextBoxFor(m => m.CNPJ, new { @class = "form-control" })
    </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Servico, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.Servico, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Register" />
        </div>
    </div>
}