@model IEnumerable<Tcc.Models.ApplicationUser>

@{
    ViewBag.Title = "AdministrarUsuarios";
}

<h2>AdministrarUsuarios</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Modificar role", "ModificarRoles", null, new { id = item.Id })
                @Html.ActionLink("Delete", "DeletarUsuario", null, new { id = item.Id })
            </td>
        </tr>
    }

</table>


